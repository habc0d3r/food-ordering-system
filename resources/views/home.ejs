      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 col-gap-12 row-gap-16">
        <% pizzas.forEach(function(pizza) {%>
          <div class="w-full md:w-64 p-6 border rounded-2xl shadow-2xl">
            <img class="h-40 mb-4 mx-auto" src="/img/<%= pizza.image %>" alt="">
            <div class="text-center">
              <h2 class="mb-4 text-lg"><%= pizza.name %></h2>
              <h3 class="mb-3">Lorem ipsum dolor sit amet consectetur adipisicing elit. Iste, consequuntur!</h3>
              <div class="flex items-center justify-around">
                <div class="flex flex-col justify-center items-center">
                  <span class="food-size italic text-sm text-green-800 mb-1">Size</span>
                  <span class="size py-1 px-4 rounded-tl-full uppercase text-xs font-bold bg-gray-300"><%= pizza.size %></span>
                </div>
                <div class="flex flex-col justify-center items-center">
                  <span class="font-bold text-2xl"><span class="text-green-600 font-bold text-base">â‚¹ </span><%= pizza.price %></span>
                </div>
              </div>
              <div class="flex items-center justify-around mt-6">
                <button data-pizza="<%= JSON.stringify(pizza) %>" class="add-to-cart py-1 px-8 rounded-full flex items-center font-bold shadow-xl">
                  <span class="">Order Now</span>
                </button>
              </div>
            </div>
          </div>
        <% }) %>
      </div>
